#!/usr/bin/env bash
# This script parses the Apache access log, groups visitors by IP and HTTP status code,
# counts occurrences, and displays the data sorted by the greatest number of occurrences.

# Use awk to group by IP and HTTP code and count occurrences
awk '{count[$1,$9]++} 
     END {for (key in count) print count[key], key}' "$1" | 
sort -nr | 
awk '{print $1, $2, $3}'
